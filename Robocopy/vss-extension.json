{
	"manifestVersion": 1,
	"id": "jungit-robocopy",
	"name": "Robocopy",
	"description": "Robocopy task wrapper for TFS/VSTS",
	"version": "1.1.0",
	"publisher": "jungeriusit",
	"public": true, 
	"baseUri": "https://github.com/chrisism/",
	"tags": [
		"copy",
		"move",
		"files",
		"folders",
		"filesystem"
	],
	"links": {
		"home": {
			"uri": "https://github.com/chrisism/VSTS.Robocopy"
		},
		"learn": {
			"uri": "https://github.com/chrisism/VSTS.Robocopy"
		},
		"support": {
			"uri": "https://github.com/chrisism/VSTS.Robocopy/issues"
		},
		"repository": {
			"uri": "https://github.com/chrisism/VSTS.Robocopy"
		}
	},
	"repository": {
		"type": "git",
		"uri": "https://github.com/chrisism/VSTS.Robocopy"
	},
	"screenshots": [
		{
			"path": "screenshot-01.jpg"
		},
		{
			"path": "screenshot-02.jpg"
		}
	],
	"branding": {
		"color": "rgb(157, 140, 255)",
		"theme": "light"
	},
	"scopes": [],
	"targets": [
		{
			"id": "Microsoft.VisualStudio.Services"
		}
	],
	"categories": [
		"Build and release"
	],
	"icons": {
		"default": "img/logo.png"
	},
	"files": [
		{ "path": "Task" },
		{ "path": "BackupTask" }
	],
	"content": {
		"details": {
			"path": "overview.md"
		},
		"license": {
			"path": "license.md"
		}
	},
	"contributions": [
		{
			"id": "jungit-robocopy",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "Task"
			}
		},
		{
			"id": "jungit-robobackup",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "BackupTask"
			}
		}
	]
}